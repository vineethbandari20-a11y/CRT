a,b=map(int,input().split())
op=input()
match op:
    case '+':
        print(a+b)
    case '-':
        print(a-b)
    case '*':
        print(a*b)
    case '/':
        if b!= 0:
            print(a/b)
        else:
            print("Error: Division by zero is not allowed.")
    case _:
        print("Invalid operator")
